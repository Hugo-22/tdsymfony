{% extends 'base.html.twig' %}

{% block title %}Admin
{% endblock %}

{% block body %}

	<table>
		<thead>
			<tr>
				<th>Email</th>
				<th>Note</th>
				<th>Commentaire</th>
			</tr>
		</thead>
		<tbody>
			{% for eval in evals %}
				<tr>
					<td>
						{{eval.email}}
					</td>
					<td>
						{{eval.note}}
					</td>
					<td>
						{{eval.commentaire}}
					</td>
				</tr>
			{% endfor %}
		</tbody>
	</table>
{% endblock %}
